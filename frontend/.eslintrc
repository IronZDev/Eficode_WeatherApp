{
  "parser": "babel-eslint",
  "extends": ["plugin:react/recommended", "airbnb"],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "semi": ["error", "always"],
    "comma-dangle": ["error", "always-multiline"],
    "quotes": ["error", "single"],
    "no-multi-spaces": ["error"]
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "fetch": false,
    "document": true
  }
}
