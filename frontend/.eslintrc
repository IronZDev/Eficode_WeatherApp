{
  "parser": "babel-eslint",
  "extends": ["plugin:react/recommended", "plugin:jest/recommended",
  "airbnb"],
  "env": {
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "semi": ["error", "always"],
    "comma-dangle": ["error", "always-multiline"],
    "quotes": ["error", "single"],
    "no-multi-spaces": ["error"],
    "no-alert":"off",
    "react/prop-types":"off",
    "max-len": ["error", {"code": 140, "ignoreUrls": true, "ignoreStrings": true, "ignoreComments": true}],
    "camelcase":"off"
  },
  "plugins": [
    "react", "jest"
  ],
  "globals": {
    "fetch": false,
    "document": true
  }
}
